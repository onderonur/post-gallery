{
  "name": "api",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "@graphql-codegen/cli": "^1.13.3",
    "@graphql-codegen/introspection": "^1.13.3",
    "@graphql-codegen/typescript": "^1.13.3",
    "@graphql-codegen/typescript-resolvers": "^1.13.3",
    "@types/express": "^4.17.4",
    "@types/express-useragent": "^1.0.0",
    "@types/graphql": "^14.5.0",
    "@types/graphql-upload": "^8.0.3",
    "@types/helmet": "0.0.45",
    "@types/lodash": "^4.14.150",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^13.13.2",
    "@types/object-hash": "^1.3.1",
    "@types/sharp": "^0.25.0",
    "@types/uuid": "^7.0.3",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.7",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-node": "^8.9.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.3.9",
    "apollo-datasource": "^0.7.0",
    "apollo-server-express": "^2.12.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "class-validator": "^0.12.2",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-useragent": "^1.0.13",
    "google-auth-library": "^6.0.0",
    "graphql": "^14.6.0",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "nanoid": "^3.1.3",
    "object-hash": "^2.0.3",
    "pg": "^8.0.3",
    "reflect-metadata": "^0.1.10",
    "sharp": "^0.25.2",
    "typeorm": "^0.2.24",
    "uuid": "^7.0.3"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only --inspect -- src/index.ts",
    "start": "cross-env NODE_ENV=development ts-node src/index.ts",
    "build": "cross-env NODE_ENV=production tsc -p tsconfig.production.json",
    "preprod": "npm run build",
    "prod": "cross-env NODE_ENV=production node dist/index.js",
    "clean": "rimraf dist",
    "typeorm": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm/cli.js",
    "gql-gen": "graphql-codegen --config codegen.yml",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint:fix": "eslint --fix --max-warnings 0 --ext .js,.jsx,.ts,.tsx src/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run typecheck && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run lint:fix"
    ]
  }
}
