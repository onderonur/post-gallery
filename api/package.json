{
  "name": "api",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "@graphql-codegen/cli": "^1.11.2",
    "@graphql-codegen/introspection": "^1.11.2",
    "@graphql-codegen/typescript": "^1.11.2",
    "@graphql-codegen/typescript-resolvers": "^1.11.2",
    "@types/express": "^4.17.2",
    "@types/graphql": "^14.5.0",
    "@types/graphql-upload": "^8.0.3",
    "@types/lodash": "^4.14.149",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^13.5.0",
    "@types/sharp": "^0.24.0",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "apollo-datasource": "^0.6.4",
    "apollo-server-express": "^2.9.16",
    "class-validator": "^0.11.0",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "lodash": "^4.17.15",
    "nanoid": "^2.1.10",
    "pg": "^7.17.1",
    "reflect-metadata": "^0.1.10",
    "sharp": "^0.24.0",
    "typeorm": "0.2.22"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --inspect --respawn --transpileOnly src/index.ts",
    "prod": "cross-env NODE_ENV=production node dist/index.js",
    "build": "cross-env NODE_ENV=production tsc -p tsconfig.production.json",
    "start": "cross-env NODE_ENV=production ts-node src/index.ts",
    "clean": "rimraf dist",
    "typeorm": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm/cli.js",
    "db:seed": "cross-env NODE_ENV=development ts-node src/seed.ts",
    "gql:generate": "graphql-codegen --config codegen.yml",
    "eslint:fix": "node_modules/.bin/eslint --fix . --ext .ts,.js"
  }
}
