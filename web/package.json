    "clean": "rimraf build",
    "typecheck": "tsc --noEmit",
    "lint:fix": "eslint --fix --ext .ts,.tsx,.js,.jsx ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
